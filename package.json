  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:api\" \"yarn dev:web\" \"yarn dev:mobile\"",
    "dev:api": "cd src/apps/api && yarn dev",
    "dev:web": "cd src/apps/web && yarn dev",
    "dev:mobile": "cd src/apps/mobile && yarn start",
    "build": "concurrently \"yarn build:api\" \"yarn build:web\"",
    "build:api": "cd src/apps/api && yarn build",
    "build:web": "cd src/apps/web && yarn build",
    "test": "concurrently \"yarn test:api\" \"yarn test:web\"",
    "test:api": "cd src/apps/api && yarn test",
    "test:web": "cd src/apps/web && yarn test",
    "lint": "concurrently \"yarn lint:api\" \"yarn lint:web\"",
    "lint:api": "cd src/apps/api && yarn lint",
    "lint:web": "cd src/apps/web && yarn lint",
    "format": "concurrently \"yarn format:api\" \"yarn format:web\"",
    "format:api": "cd src/apps/api && yarn format",
    "format:web": "cd src/apps/web && yarn format",
    "prisma:generate": "cd src/apps/api && yarn prisma generate",
    "prisma:migrate": "cd src/apps/api && yarn prisma migrate dev",
    "prisma:studio": "cd src/apps/api && yarn prisma studio"
  },
